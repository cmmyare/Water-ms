	<queryString language="SQL">
		<![CDATA[SELECT p.people_no,p.name,p.tell,d.district, d.village, d.zone, 
       SUM(r.amount) AS Total_Paid
FROM people p, accounts a, receipt r,
address d where d.add_no=p.add_no and a.acc_no=r.acc_no and p.people_no=r.people_no 
