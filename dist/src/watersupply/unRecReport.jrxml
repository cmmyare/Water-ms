	<queryString language="SQL">
		<![CDATA[SELECT DISTINCT p.people_no, p.name, p.tell,(Current_rate - ex_rate)*rate as Balance
FROM Bills b
JOIN customer_homes ch ON ch.cus_h_no = b.cus_H_no
LEFT JOIN receipt r ON ch.cust_no = r.people_no
JOIN People p ON ch.cust_no = p.people_no
WHERE r.people_no IS NULL]]>
